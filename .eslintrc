{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "rules": {
    "semi": "off",
    "indent": [
      "error",
      4
    ],
    "arrow-parens": "off",
    "comma-dangle": [
      "error",
      "only-multiline"
    ],
    "require-jsdoc": "off",
    "max-len": "off",
    "operator-linebreak": "off",
    "no-debugger": "off"
  }
}
